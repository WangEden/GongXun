        <catchG>cthg</catchG>
        <catchB>cthb</catchB>
        <!-- 进行放置 红/绿/蓝 -->
        <setR>setR</setR> 
        <setG>setG</setG>
        <setB>setB</setB>
        <!-- 色环间切换 -->
        <moveRing>move</moveRing>

        <!-- 光流数据更新 -->
        <update>updt</update>
    </message>
